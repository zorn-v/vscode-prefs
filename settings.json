{
    "git.autofetch": true,
    "editor.fontSize": 13,
    "telemetry.enableTelemetry": false,
    "workbench.iconTheme": "file-icons",
    "workbench.colorTheme": "Atom One Light",
    "workbench.colorCustomizations": {
        "terminal.background": "#222",
        "terminal.selectionBackground": "#ffffff50",
        "terminal.foreground": "#ddd",
        "terminal.ansiWhite": "#ddd"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": ["source.php variable", "punctuation.definition.variable.php"],
                "settings": {
                    "foreground": "#00c"
                }
            },
            {
                "scope": "storage.modifier.php",
                "settings": {
                    "foreground": "#000",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "source.php meta.function.parameter.typehinted support.class",
                    "source.php meta.function support.class",
                ],
                "settings": {
                    "foreground": "#A626A4",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "support.function.magic.php",
                    "source.php storage",
                    "source.php keyword.control",
                    "support.type.property-name.css"
                ],
                "settings": {
                    "fontStyle": "bold"
                }
            }
        ]
    },
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "diffEditor.renderSideBySide": true,
    "git.enableSmartCommit": true,
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": true,
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "workbench.startupEditor": "none",
    "files.enableTrash": false,
    "window.newWindowDimensions": "inherit",
    "diffEditor.ignoreTrimWhitespace": false,
    "[vue]": {
        "editor.tabSize": 2
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "php.suggest.basic": false,
    "editor.selectionClipboard": false,
    "extensions.ignoreRecommendations": false,
    "github.gitAuthentication": false,
    "editor.columnSelection": false,
    "vetur.ignoreProjectWarning": true,
}
